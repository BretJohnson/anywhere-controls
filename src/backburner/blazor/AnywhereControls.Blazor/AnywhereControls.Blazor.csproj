    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
    <RootNamespace>AnywhereUI.Blazor</RootNamespace>

    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>..\AnywhereUI.snk</AssemblyOriginatorKeyFile>
