<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:a="http://schemas.microsoft.com/anywhere-conrols/2023"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaSamplesHost.MainWindow"
        Title="AvaloniaSamplesHost">
    <StackPanel HorizontalAlignment="Stretch" Margin="20">
        <Label Content=".NET Anywhere Controls Examples" FontSize="24"/>
        <Grid Name="Examples" HorizontalAlignment="Left" Margin="10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
    </StackPanel>
</Window>
