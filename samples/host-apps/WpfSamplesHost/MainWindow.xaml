<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:s="http://schemas.microsoft.com/anywhere-controls/2023"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="WpfHost.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <s:VerticalStack HorizontalAlignment="Stretch" Margin="20">
        <Label Content=".NET Anywhere Controls Examples" FontSize="24"/>
        <Grid Name="Examples" HorizontalAlignment="Left" Margin="10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
        </Grid>
    </s:VerticalStack>
</Window>
